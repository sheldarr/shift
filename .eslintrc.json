{
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "esprima",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-alert": 0,
        "no-array-constructor": 2,
        "no-bitwise": 0,
        "no-caller": 1,
        "no-case-declarations": 2,
        "no-catch-shadow": 0,
        "no-class-assign": 2,
        "no-cond-assign": 2,
        "no-confusing-arrow": 1,
        "no-console": 1,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-continue": 0,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 0,
        "no-dupe-class-members": 2,
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-duplicate-case": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-function": 1,
        "no-empty-pattern": 2,
        "no-eq-null": 1,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-boolean-cast": 2,
        "no-extra-label": 1,
        "no-extra-parens": 0,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 1,
        "no-func-assign": 2,
        "no-implicit-coercion": 1,
        "no-implicit-globals": 1,
        "no-implied-eval": 2,
        "no-inline-comments": 0,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-invalid-this": 2,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-label-var": 0,
        "no-labels": 1,
        "no-lone-blocks": 1,
        "no-lonely-if": 1,
        "no-loop-func": 1,
        "no-mixed-requires": 0,
        "no-mixed-spaces-and-tabs": 2,
        "linebreak-style": 0,
        "no-multi-spaces": 1,
        "no-multi-str": 0,
        "no-multiple-empty-lines": 2,
        "no-native-reassign": 2,
        "no-negated-condition": 1,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 1,
        "no-new": 2,
        "no-new-func": 1,
        "no-new-object": 1,
        "no-new-require": 2,
        "no-new-symbol": 2,
        "no-new-wrappers": 1,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 0,
        "no-param-reassign": 2,
        "no-path-concat": 0,
        "no-plusplus": 0,
        "no-process-env": 0,
        "no-process-exit": 0,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-restricted-globals": 0,
        "no-restricted-imports": 0,
        "no-restricted-modules": 0,
        "no-restricted-syntax": 0,
        "no-return-assign": 1,
        "no-script-url": 1,
        "no-self-assign": 2,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-shadow": 0,
        "no-shadow-restricted-names": 2,
        "no-whitespace-before-property": 1,
        "no-spaced-func": 0,
        "no-sparse-arrays": 2,
        "no-sync": 0,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-this-before-super": 2,
        "no-throw-literal": 1,
        "no-undef": 2,
        "no-undef-init": 1,
        "no-undefined": 1,
        "no-unexpected-multiline": 2,
        "no-underscore-dangle": 1,
        "no-unmodified-loop-condition": 1,
        "no-unneeded-ternary": 1,
        "no-unreachable": 2,
        "no-unused-expressions": 1,
        "no-unused-labels": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 1,
        "no-useless-call": 1,
        "no-useless-concat": 1,
        "no-useless-constructor": 1,
        "no-void": 0,
        "no-var": 1,
        "no-warning-comments": 1,
        "no-with": 1,
        "no-magic-numbers": 0,
        "array-bracket-spacing": 1,
        "array-callback-return": 1,
        "arrow-body-style": 1,
        "arrow-parens": 1,
        "arrow-spacing": 1,
        "accessor-pairs": 0,
        "block-scoped-var": 0,
        "block-spacing": 1,
        "brace-style": 1,
        "callback-return": 1,
        "camelcase": 0,
        "comma-dangle": 2,
        "comma-spacing": 1,
        "comma-style": 1,
        "complexity": [
            0,
            11
        ],
        "computed-property-spacing": 1,
        "consistent-return": 0,
        "consistent-this": 1,
        "constructor-super": 2,
        "curly": 1,
        "default-case": 1,
        "dot-location": 0,
        "dot-notation": 1,
        "eol-last": 1,
        "eqeqeq": 1,
        "func-names": 0,
        "func-style": 0,
        "generator-star-spacing": 1,
        "global-require": 2,
        "guard-for-in": 0,
        "handle-callback-err": 1,
        "id-length": 0,
        "indent": 1,
        "init-declarations": 0,
        "jsx-quotes": 1,
        "key-spacing": 1,
        "keyword-spacing": 1,
        "lines-around-comment": 1,
        "max-depth": 0,
        "max-len": 0,
        "max-nested-callbacks": 0,
        "max-params": 0,
        "max-statements": 0,
        "new-cap": 1,
        "new-parens": 2,
        "newline-after-var": 1,
        "newline-per-chained-call": 0,
        "object-curly-spacing": [
            1,
            "never"
        ],
        "object-shorthand": 1,
        "one-var": 0,
        "one-var-declaration-per-line": 0,
        "operator-assignment": 1,
        "operator-linebreak": 1,
        "padded-blocks": 0,
        "prefer-arrow-callback": 1,
        "prefer-const": 1,
        "prefer-reflect": 0,
        "prefer-rest-params": 1,
        "prefer-spread": 1,
        "prefer-template": 1,
        "quote-props": 0,
        "quotes": [
            1,
            "single"
        ],
        "radix": 1,
        "id-match": 0,
        "id-blacklist": 0,
        "require-jsdoc": 0,
        "require-yield": 0,
        "semi": 2,
        "semi-spacing": 1,
        "sort-vars": 0,
        "sort-imports": 1,
        "space-before-blocks": 1,
        "space-before-function-paren": 1,
        "space-in-parens": 1,
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "spaced-comment": 1,
        "strict": 0,
        "template-curly-spacing": 1,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "vars-on-top": 0,
        "wrap-iife": 1,
        "wrap-regex": 1,
        "yield-star-spacing": 0,
        "yoda": 1,
        "react/display-name": 0,
        "react/forbid-prop-types": 0,
        "react/no-danger": 1,
        "react/no-deprecated": 1,
        "react/no-did-mount-set-state": 0,
        "react/no-did-update-set-state": 1,
        "react/no-direct-mutation-state": 1,
        "react/no-is-mounted": 2,
        "react/no-multi-comp": 1,
        "react/no-set-state": 0,
        "react/no-string-refs": 1,
        "react/no-unknown-property": 1,
        "react/prefer-es6-class": 0,
        "react/prop-types": 1,
        "react/react-in-jsx-scope": 1,
        "react/require-extension": 1,
        "react/self-closing-comp": 1,
        "react/sort-comp": 1,
        "react/sort-prop-types": 1,
        "react/wrap-multilines": 1,
        "react/jsx-boolean-value": 1,
        "react/jsx-closing-bracket-location": 1,
        "react/jsx-curly-spacing": 1,
        "react/jsx-handler-names": 0,
        "react/jsx-indent-props": 1,
        "react/jsx-indent": 1,
        "react/jsx-key": 1,
        "react/jsx-max-props-per-line": [
            1,
            {
                "maximum": 3
            }
        ],
        "react/jsx-no-bind": 0,
        "react/jsx-no-duplicate-props": 1,
        "react/jsx-no-literals": 1,
        "react/jsx-no-undef": 1,
        "react/jsx-pascal-case": 1,
        "react/jsx-sort-props": 1,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1
    }
}
